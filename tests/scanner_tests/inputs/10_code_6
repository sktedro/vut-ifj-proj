require "ifj21" --Tento program by nemal by≈• spravny

function program()
	local y : integer = 20
	if 1 1 then -- tu chyba relacny operator, semanticka chyba od parseru
		y = 10
		write(y)
	else
		write(y)
	end
	write(y) -- vypise 10

	local i : integer = 1
	while i <= 10 do
		local i : integer = i + 5
		i = i + 1
		write(i)
		-- nekonecny cyklus (7...)
	end
end

program()
