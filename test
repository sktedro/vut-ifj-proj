.ifjcode21

# New function definition
JUMP *label__0
LABEL main0

PUSHFRAME

# Jumping to variable declaration
JUMP *label__1

# Jump back here from declarations
LABEL *label__2


JUMP *label_EXPR2

LABEL *label_EXPR0
DEFVAR LF@?tmpvar_0
MOVE LF@?tmpvar_0 int@1
MOVE LF@%var_a_1 LF@?tmpvar_0
#IDEME GENEROVAŤ 2 NUM 0
#NUM 1
JUMP *label_EXPREND0

LABEL *label_EXPR1
DEFVAR LF@?tmpvar_1
MOVE LF@?tmpvar_1 int@2
MOVE LF@%var_b_1 LF@?tmpvar_1
#IDEME GENEROVAŤ 1 NUM 0
#NUM 1
#NUM 2
#TUIUUUUUUUUUUUUUUUUUU
#--------------------------------aaaa
JUMP *label_EXPR0

LABEL *label_EXPR2
#--------------------------------aaaa
DEFVAR LF@?tmpvar_2
MOVE LF@?tmpvar_2 int@3
MOVE LF@%var_c_1 LF@?tmpvar_2
#---------------------------------mm
JUMP *label_EXPR1

LABEL *label_EXPREND0
#---------------------------------mm
JUMP *label__3

# Variable declarations
LABEL *label__1
DEFVAR LF@%var_a_1
DEFVAR LF@%var_b_1
DEFVAR LF@%var_c_1

# Jump from var declarations
JUMP *label__2

# Skip variable declaration
LABEL *label__3

WRITE LF@%var_a_1
WRITE LF@%var_b_1
WRITE LF@%var_c_1

POPFRAME
RETURN
# Function definition done

LABEL *label__0

# Calling a function from the global frame

# Calling a function
CREATEFRAME

# Processing function call arguments
# Processing function call arguments done

CALL main0
# Function call from the global frame done
