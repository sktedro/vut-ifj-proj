require "ifj21"

global print : function (integer, string, number) : integer, string

function g(i: integer): integer
    return i + 1
end

function f(i : integer)
    while i ~= 20 do
        i = g(i)
        write(i)
    end
end

-- function print(i : integer, s: string, n: number) : integer, string
--     write(i, s, n)
--     return x, "huhu"
-- end

f(10)
